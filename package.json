{
  "name": "@dfhernandez/commland-dielectric-js-sdk",
  "version": "0.1.3",
  "description": "SDK written in Javascript and Typescript to provide access to native APIs using Capacitor plugins",
  "author": "Joris Tirado <joris@2600hz.com>, Daniel Hernandez <fhernandez@2600hz.com> (https://github.com/shark764)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "source": "src/index",
  "files": [
    "bin",
    "build",
    "tools",
    "src",
    "lib",
    "tsconfig.base.json",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "yarn clean && tsc",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prettify": "yarn run format && yarn run lint:fix",
    "full-check": "yarn run prettify && yarn run typescript",
    "release": "release-it",
    "test": "jest --watchAll"
  },
  "keywords": [
    "utilities"
  ],
  "repository": "https://github.com/shark764/commland-dielectric-js-sdk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shark764/commland-dielectric-js-sdk/issues"
  },
  "homepage": "https://github.com/shark764/commland-dielectric-js-sdk#readme",
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@2600hz/commio-native-utilities": "^0.5.2",
    "@capacitor/browser": "^1.0.7",
    "@capacitor/core": "^3.4.3",
    "@capacitor/geolocation": "^1.3.1",
    "@commitlint/config-conventional": "^13.1.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "release-it": "^14.2.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
